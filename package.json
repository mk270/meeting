{
  "name": "meeting",
  "version": "0.0.1",
  "description": "A meeting board with WebRTC integration",
  "main": "build/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base build/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "test": "npm run coverage",
    "test-unit": "mocha --compilers js:babel-core/register test/unit/**/*.spec.js",
    "test-unit-watch": "mocha --watch --compilers js:babel-core/register test/unit/**/*.spec.js",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --timeout 30000 test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgryko/meeting.git"
  },
  "keywords": [
    "Meeting",
    "board",
    "WebRTC",
    "Video",
    "Conferencing"
  ],
  "author": "Piotr Gryko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgryko/meeting/issues"
  },
  "homepage": "https://github.com/pgryko/meeting#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.22.1",
    "html-webpack-plugin": "^2.10.0",
    "mocha": "^2.4.5",
    "nock": "^7.2.2"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "style-loader": "^0.13.0"
  }
}
