  "author": "Piotr Gryko",
  "bugs": {
    "url": "https://github.com/pgryko/meeting/issues"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "if-env": "^1.0.0",
    "material-ui": "^0.15.0-beta.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-onclickoutside": "^5.0.1",
    "react-router": "^2.0.1",
    "react-slick": "^0.12.0",
    "react-tap-event-plugin": "^1.0.0",
    "resolve-url": "^0.2.1",
    "style-loader": "^0.13.0",
    "validator": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "express-hot-loader": "0.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.23.1",
    "html-webpack-plugin": "^2.10.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0"
  },
  "homepage": "https://github.com/pgryko/meeting#readme",
  "keywords": [
    "Conferencing",
    "Meeting",
    "Video",
    "WebRTC",
    "board"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgryko/meeting.git"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.config.js --progress --inline --colors",
    "build:server": "webpack --config webpack.server.config.js --colors",
    "build:watch": "parallelshell 'npm run watch:client' 'npm run watch:server'",
    "clean": "rm -r build/*",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --timeout 30000 test/**/*.spec.js",
    "start": "npm run build && nodemon build/server.bundle.js",
    "test": "npm run coverage",
    "test-unit": "mocha --compilers js:babel-core/register test/unit/**/*.spec.js",
    "test-unit-watch": "mocha --watch --compilers js:babel-core/register test/unit/**/*.spec.js",
    "watch:client": "webpack --progress --colors --watch",
    "watch:server": "webpack --config webpack.server.config.js --colors --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  }
}
